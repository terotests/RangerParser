  "description": "Language Agnostic DSL parser",
  "main": "dist/src/NodeIterator.js",
  "types": "dist/src/NodeIterator.d.ts",
  "scripts": {
    "tsc": "tsc",
    "oldtest": "npm run tsc && ./node_modules/.bin/mocha dist/test/ --recursive --reporter spec",
    "test": "npm run tsc && jest"
  },
  "keywords": [],
  "author": "Tero Tolonen",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "chai": "^4.1.2",
    "jest": "^24.9.0",
    "mocha": "^5.2.0",
    "typescript": "^3.0.1",
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.5.6"
  },
  "jest": {
    "verbose": true,
    "testMatch": [
      "**/__tests__/**/*.[j]s?(x)",
      "**/?(*.)+(spec|test).[j]s?(x)"
